<?xml version="1.0" ?>
<robot name="ModularBot">
   <group name="chain_A">
      <chain base_link="mobile_base" tip_link="wheel_A"/>
   </group>
   <group name="chain_B">
      <chain base_link="mobile_base" tip_link="wheel_B"/>
   </group>
   <group name="chain_C">
      <chain base_link="mobile_base" tip_link="wheel_C"/>
   </group>
   <group name="chain_D">
      <chain base_link="mobile_base" tip_link="wheel_D"/>
   </group>
   <group name="chain_E">
      <chain base_link="mobile_base" tip_link="ee_E"/>
   </group>
   <group name="arms">
      <group name="chain_A"/>
      <group name="chain_B"/>
      <group name="chain_C"/>
      <group name="chain_D"/>
      <group name="chain_E"/>
