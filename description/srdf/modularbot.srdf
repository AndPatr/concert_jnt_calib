      <joint name="J_wheel_B" value="0.0"/>
      <joint name="J1_C" value="0.0"/>
      <joint name="J_wheel_C" value="0.0"/>
      <joint name="J1_D" value="0.0"/>
      <joint name="J_wheel_D" value="0.0"/>
      <joint name="J1_E" value="0.0"/>
      <joint name="J2_E" value="-0.5"/>
      <joint name="J3_E" value="0.0"/>
      <joint name="J4_E" value="0.5"/>
      <joint name="J5_E" value="0.0"/>
   </group_state>
</robot>
